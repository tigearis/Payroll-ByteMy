{
  "name": "payroll-bytemy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:production": "NODE_ENV=production next build",
    "build:verify": "node scripts/verify-production-build.js",
    "verify:production": "NODE_ENV=production node scripts/verify-production-build.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality:check": "pnpm lint:strict && pnpm format:check && pnpm type-check",
    "quality:fix": "pnpm lint:fix && pnpm format",
    "fix:unused-vars": "node scripts/fix-remaining-unused-vars.cjs",
    "lint:complete-fix": "pnpm fix:unused-vars && pnpm lint:fix && pnpm format",
    "fix:codebase": "node scripts/fix-codebase.js",
    "fix:dry-run": "node scripts/fix-codebase.js --dry-run",
    "fix:auth": "node scripts/fix-codebase.js --fix=auth --backup",
    "fix:errors": "node scripts/fix-codebase.js --fix=errors --backup",
    "fix:all": "node scripts/fix-codebase.js --fix=auth,errors --backup",
    "fix:duplication": "node scripts/fix-codebase.js --fix=duplication",
    "generate": "graphql-codegen --config config/codegen.ts",
    "codegen": "graphql-codegen --config config/codegen.ts",
    "codegen:watch": "graphql-codegen --config config/codegen.ts --watch",
    "codegen:debug": "graphql-codegen --config config/codegen.ts --verbose",
    "codegen:dry-run": "graphql-codegen --config config/codegen.ts --check",
    "get-schema": "graphql-codegen --config config/codegen.ts --require dotenv/config --schema $NEXT_PUBLIC_HASURA_GRAPHQL_URL --header 'x-hasura-admin-secret:$HASURA_ADMIN_SECRET' --output shared/schema/schema.graphql --plugin schema-ast",
    "validate:naming": "node scripts/validate-naming-conventions.js",
    "validate:naming:fix": "node scripts/fix-naming-conventions.js",
    "validate:naming:dry-run": "tsx scripts/validate-case-conventions.ts --fix --no-dry-run",
    "fix:permissions": "node scripts/fix-hasura-permissions.js",
    "fix:permissions:dry-run": "node scripts/fix-hasura-permissions.js --dry-run",
    "test:dev": "node scripts/dev-test.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:users:create": "node scripts/create-test-users.js create",
    "test:users:list": "node scripts/create-test-users.js list",
    "test:users:delete": "node scripts/create-test-users.js delete",
    "test:users:recreate": "node scripts/create-test-users.js recreate",
    "test:users:update-passwords": "node scripts/update-test-passwords.js",
    "test:users:sync": "node scripts/sync-test-users.js sync",
    "test:users:sync:direct": "node scripts/sync-test-users.js direct",
    "test:users:sync:check": "node scripts/sync-test-users.js check",
    "test:users:sync:enhanced": "node scripts/sync-test-users-enhanced.js",
    "test:setup": "node scripts/setup-e2e-tests.js",
    "test:verify": "node scripts/verify-e2e-setup.js",
    "test:data:seed": "node scripts/seed-minimal-data.js seed",
    "test:data:clean": "node scripts/seed-minimal-data.js clean",
    "test:data:reseed": "node scripts/seed-minimal-data.js reseed",
    "test:data:generate": "node scripts/generate-fake-data.js generate",
    "test:data:quick": "node scripts/quick-seed.js",
    "test:data:real": "node scripts/seed-real-data.js seed",
    "test:data:real:clean": "node scripts/seed-real-data.js clean",
    "test:data:real:reseed": "node scripts/seed-real-data.js reseed",
    "test:data:minimal": "node scripts/seed-minimal-data.js seed",
    "test:data:minimal:clean": "node scripts/seed-minimal-data.js clean",
    "test:data:minimal:reseed": "node scripts/seed-minimal-data.js reseed",
    "hasura:console": "cd hasura && hasura console",
    "hasura:migrate": "cd hasura && hasura migrate apply",
    "hasura:metadata": "cd hasura && hasura metadata apply",
    "hasura:seeds": "cd hasura && hasura seeds apply",
    "prepare": "husky install || true"
  },
